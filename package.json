{
  "name": "@peak-ai/query-builder",
  "version": "0.2.2",
  "description": "Validate AIS-conformant query trees and serialise them as SQL strings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "format:check": "prettier --check jest.config.js 'src/**/*.ts'",
    "format:write": "prettier --write jest.config.js 'src/**/*.ts'",
    "build": "tsc -p .",
    "prepublishOnly": "yarn lint && yarn format:check && yarn build && yarn test",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peak-ai/query-builder.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sql",
    "query",
    "builder",
    "where",
    "clause"
  ],
  "author": "James Wright <james.wright@peak.ai>",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/peak-ai/query-builder/issues"
  },
  "homepage": "https://github.com/peak-ai/query-builder#readme",
  "devDependencies": {
    "@types/jest": "25.1.3",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "moment": "2.24.0",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.2"
  },
  "dependencies": {}
}
